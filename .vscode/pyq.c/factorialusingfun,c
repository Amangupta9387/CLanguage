#include<stdio.h>
int factorial(int n);
int main(){
int n,fact=1;
printf("enter the number");
scanf("%d",&n);
  fact=factorial(n);
    return 0;
}
int factorial(int n){
    int fact=1;
    for(int i=1;i<n;i++){
        fact=fact*i;
    }
return fact;
}

